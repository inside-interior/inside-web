	body
		// section.loading-page Loading ...
		span.menu-btn.js-menu-btn menu
		nav.nav.js-nav
			.nav__links
				a.nav__links__link.js-nav-link(data-scroll='', href='#about', data-i18n='nav.about')
				a.nav__links__link.js-nav-link(data-scroll='', href='#expertise', data-i18n='nav.expertise')
				a.nav__links__link.js-nav-link(data-scroll='', href='#projects', data-i18n='nav.projects')
				a.nav__links__link.js-nav-link(data-scroll='', href='#contact', data-i18n='nav.contact')
			p.nav__copyright='Â© 2017 '
				span Inside Interior
		main
			header.intro.js-intro
				svg.intro__logo
					use(xlink:href='images/icons.svg#inside_logo', data-i18n-link='xlink:href')
				.intro__intro
					h1.intro__title Inside
					h2.intro__tagline Where design comes alive
				.intro__lang
					a(href='/', class='lang__link lang__link--id', aria-label='Bahasa Indonesia', title='Bahasa Indonesia')
						svg(class='lang__icon')
							use(xlink:href='images/icons.svg#flag_id', data-i18n-link='xlink:href')
					span.lang__separator |
					a(href='/en', class='lang__link lang__link--en', aria-label='English', title='English')
						svg(class='lang__icon')
							use(xlink:href='images/icons.svg#flag_us', data-i18n-link='xlink:href')
			section#about.section
				.section__content
					h2.section__title.js-section-title(data-i18n='about.title')
					p.about__content.js-about-us(data-i18n='about.content')
			-
				var expertises = [
					{
						title  : 'expertise.item_1.title',
						detail : 'expertise.item_1.detail'
					},
					{
						title  : 'expertise.item_2.title',
						detail : 'expertise.item_2.detail'
					},
					{
						title  : 'expertise.item_3.title',
						detail : 'expertise.item_3.detail'
					}
				]
			section#expertise.section
				.section__content
					h2.section__title.js-section-title(data-i18n='expertise.title')
					ul.expertises
						each expertise, index in expertises
							li.expertise-wrapper.js-expertise(class= index == 0 ? 'js-last-hover' : '')
								.expertise
									h3.expertise__title(data-i18n=expertise.title)
									p.expertise__detail(data-i18n=expertise.detail)
			section#projects.section.section--projects
				.section__content
					h2.section__title.js-section-title(data-i18n='projects.title')
					each projects in locals
						h3.projects-category.js-project-category(data-i18n=projects.category)
						ul.projects.js-projects
							each project in projects.projects
								li.project-wrapper.js-project
									a(href=project.permalink)
										- var img_src = 'images/projects/' + project.permalink + '/' + project.homepage_img

										- var srcset_str = img_src + '-sq-xs.jpg 175w,'
										- srcset_str    += img_src + '-sq-sm.jpg 310w,'
										- srcset_str    += img_src + '-sq-me.jpg 445w,'
										- srcset_str    += img_src + '-sq-la.jpg 580w,'
										- srcset_str    += img_src + '-sq-xl.jpg 715w,'
										- srcset_str    += img_src + '-sq-xxl.jpg 850w'

										- var sizes_str = '(min-width: 34.75em) calc(50vw - 3rem), '
										- sizes_str    += '(min-width: 59em) calc((100vw - 3rem) / 3), '
										- sizes_str    += '75vw'
										figure.project.lazyload_wrapper
											img.project__img.lazyload(data-srcset=srcset_str,
											                          data-sizes='auto',
											                          data-src=img_src + '-sq-me.jpg',
											                          alt=project.title,
											                          data-i18n-link='data-srcset,data-src')
											figcaption.project__title= project.title
										noscript
											figure.project
												img.project__img(srcset=srcset_str,
												                 sizes=sizes_str,
												                 src=img_src + '-sq-me.jpg',
												                 alt=project.title,
												                 data-i18n-link='srcset,src')
												figcaption.project__title= project.title
			section#contact.section
				.section__content
					h2.section__title.js-section-title(data-i18n='contact.title')
					p.contact-greet.js-contact-greet(data-i18n='contact.subtitle')
					.contacts
						.contact.js-contact
							svg.contact__logo
								use(xlink:href='images/icons.svg#phone', data-i18n-link='xlink:href')
							.contact__content
								a.contact__link(href='tel:+6285-236-727-777', onclick='goog_report_conversion("tel:+6285-236-727-777")') +6285-236-727-777
								p.contact__name Randy Eka Pranoto S. T.
								a.contact__link(href='tel:+6281-251-220-220', onclick='goog_report_conversion("tel:+6281-251-220-220")') +6281-251-220-220
								p.contact__name Agung Suryajaya Putra S. T.
						.contact.js-contact
							svg.contact__logo
								use(xlink:href='images/icons.svg#pinpoint', data-i18n-link='xlink:href')
							.contact__content
								a.contact__link(href='https://www.google.com/maps/place/Inside+Interior/@-7.2786331,112.6955051,17z/data=!3m1!4b1!4m5!3m4!1s0x0:0x4aae846bf4ada467!8m2!3d-7.2786331!4d112.6976938', target='_blank') Jl. Darmo Baru Barat VII/49
								p.contact__sub-detail(data-i18n='contact.sby_office')
								a.contact__link(href='https://www.google.com/maps/?q=-7.272737,112.659813', target='_blank') Jl. Raya Kuwukan No. 107 Kav. 4-5
								p.contact__sub-detail(data-i18n='contact.sby_workshop')
						.contact.js-contact
							svg.contact__logo
								use(xlink:href='images/icons.svg#mail', data-i18n-link='xlink:href')
							.contact__content
								a.contact__link(href='mailto:info@insideinterior.net', onclick='goog_report_conversion("mailto:info@insideinterior.net")') info@insideinterior.net
		footer.footer
			.footer__logo-wrapper
				a.footer__soc-med-link(href='https://www.instagram.com/inside.interior', target='_blank')
					svg.footer__soc-med-logo
						use(xlink:href='images/icons.svg#instagram', data-i18n-link='xlink:href')
				a.footer__soc-med-link(href='https://www.facebook.com/InsideStudioDesign', target='_blank')
					svg.footer__soc-med-logo
						use(xlink:href='images/icons.svg#fb', data-i18n-link='xlink:href')
			p.footer__copyright Â© 2017 Inside Interior. All rights reserved.
